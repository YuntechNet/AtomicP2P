zone "yunnms.lalala.org" {
    type master;
    file "/etc/bind/db.yunnms.lalala.org";
};

zone "yunnms.test.org" {
    type master;
    file "/etc/bind/db.yunnms.test.org";
};

zone "1.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/db.192.168.1";
};

zone "2.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/db.192.168.2";
};